{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "pastiubogdan/multi-client:1.0.0",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "server",
      "image": "pastiubogdan/multi-server:1.0.0",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "worker",
      "image": "pastiubogdan/multi-worker:1.0.0",
      "hostname": "worker",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "pastiubogdan/multi-nginx:1.0.0",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "server"]
    }
  ]
}